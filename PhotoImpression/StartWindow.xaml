<Window x:Class="PhotoImpression.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controll="clr-namespace:PhotoImpression.ViewComponents"
        Title="Photo Impression" Height="Auto" Width="Auto" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>

        <controll:LeftMenuPanel Grid.Column="0" />
        <ContentPresenter x:Name="rightPanel" Grid.Column="1" />
    </Grid>
</Window>
